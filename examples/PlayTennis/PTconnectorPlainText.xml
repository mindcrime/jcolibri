<?xml version="1.0" encoding="UTF-8"?><Connector xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xml\connector.xsd">    <CaseBase>jcolibri.cbrcase.BasicCBRCaseBase</CaseBase>		<PlainText>	    <FilePath>examples\PlayTennis\PTcasebase.txt</FilePath>		    <Delimiters> </Delimiters>			<Mappings>			<CaseIdColumn>caseId</CaseIdColumn>			<Map>				<ColumnName>raining</ColumnName>				<Relation>has-raining</Relation>					<Weight>1.0</Weight>				<Individual>RAINING</Individual>				<ColumnType>Boolean</ColumnType>				</Map>			<Map>				<ColumnName>month</ColumnName>				<Relation>has-month</Relation>					<Weight>1.0</Weight>				<Individual>MONTH</Individual>				<ColumnType>String</ColumnType>				</Map>			<Map>				<ColumnName>last_match</ColumnName>				<Relation>has-last_match</Relation>					<Weight>1.0</Weight>				<Individual>LAST_MATCH</Individual>				<ColumnType>Integer</ColumnType>				</Map>			<Map>				<ColumnName>result</ColumnName>				<Relation>has-result</Relation>					<Weight>1.0</Weight>				<Individual>RESULT</Individual>				<ColumnType>Boolean</ColumnType>				</Map>		</Mappings>		</PlainText></Connector>